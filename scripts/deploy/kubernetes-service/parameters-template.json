{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "subscriptionId": {
            "value": "%subscriptionId%"
        },
        "resourceGroup": {
            "value": "%baseName%"
        },
        "resourceName": {
            "value": "%baseName%"
        },
        "location": {
            "value": "westus"
        },
        "dnsPrefix": {
            "value": "%baseName%"
        },
        "agentCount": {
            "value": 5
        },
        "agentVMSize": {
            "value": "Standard_DS2_v2"
        },
        "servicePrincipalClientId": {
            "value": "%spAppId%"
        },
        "servicePrincipalClientSecret": {
            "value": "%spPassword%"
        },
        "kubernetesVersion": {
            "value": "1.11.3"
        },
        "workspaceName": {
            "value": "%baseName%-workspace"
        },
        "omsWorkspaceId": {
            "value": "/subscriptions/%subscriptionId%/resourceGroups/DefaultResourceGroup-EUS/providers/Microsoft.OperationalInsights/workspaces/%baseName%-workspace"
        },
        "workspaceRegion": {
            "value": "eastus"
        },
        "enableHttpApplicationRouting": {
            "value": true
        },
        "networkPlugin": {
            "value": "azure"
        },
        "enableRBAC": {
            "value": false
        },
        "networkName": {
            "value": "%networkName%"
        },
        "vnetSubnetID": {
            "value": "/subscriptions/%subscriptionId%/resourceGroups/%baseName%/providers/Microsoft.Network/virtualNetworks/%baseName%-vnet/subnets/default"
        },
        "serviceCidr": {
            "value": "10.0.0.0/16"
        },
        "dnsServiceIP": {
            "value": "10.0.0.10"
        },
        "dockerBridgeCidr": {
            "value": "172.17.0.1/16"
        },
        "principalId": {
            "value": "%principalId%"
        }
    }
}